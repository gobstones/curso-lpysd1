procedure MoverAl_(dir) { if (puedeMover(dir)) { Mover(dir) } }

/*@ATTIRE@block_icon@data:image/png;base64,@*/
procedure Pintar() { 
   if (aux_laCeldaEstáVacía())
     { AuxPintar() }
}

procedure AuxPintar() {
  foreach c in [minColor()..maxColor()] { Poner(c) } 
}


function aux_laCeldaEstáVacía() { 
   return (not hayBolitas(Azul) && not hayBolitas(Negro) && not hayBolitas(Rojo) && not hayBolitas(Verde))
}
